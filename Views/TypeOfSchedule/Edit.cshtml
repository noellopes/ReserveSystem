@model ReserveSystem.Models.TypeOfSchedule

<h2>Edit Type of Schedule</h2>

<form asp-action="Edit" method="post">
    <input type="hidden" asp-for="TypeOfScheduleId" />
    <div class="form-group">
        <label for="TypeOfScheduleName">Schedule Name</label>
        <input asp-for="TypeOfScheduleName" class="form-control" />
        <span asp-validation-for="TypeOfScheduleName" class="text-danger"></span>
    </div>
    <button type="submit" class="btn btn-primary">Save</button>
</form>