@model IEnumerable<ReserveSystem.Models.Cleaning_Schedule>

@{
    ViewData["Title"] = "Index";
}

<h1>Index</h1>

<p>
    <a asp-action="Create">Create New</a>
</p>
<table class="table">
    <thead>
        <tr>
            <th>
                @Html.DisplayNameFor(model => model.RoomBookingId)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.client)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.staffMembers)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.DateServices)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.StartTime)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.EndTime)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.CleaningDone)
            </th>
            <th></th>
        </tr>
    </thead>
    <tbody>
@foreach (var item in Model) {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.RoomBookingId)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.client.Client_Adress)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.staffMembers.StaffDriversLicense)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.DateServices)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.StartTime)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.EndTime)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.CleaningDone)
            </td>
            <td>
                <a asp-action="Edit" asp-route-id="@item.CleaningScheduleId">Edit</a> |
                <a asp-action="Details" asp-route-id="@item.CleaningScheduleId">Details</a> |
                <a asp-action="Delete" asp-route-id="@item.CleaningScheduleId">Delete</a>
            </td>
        </tr>
}
    </tbody>
</table>
