@model IEnumerable<ReserveSystem.Models.Consumptions>

@{
    ViewData["Title"] = "Index";
}

<h1>Index</h1>

<p>
    <a asp-action="Create">Create New</a>
</p>
<table class="table">
    <thead>
        <tr>
            <th>
                @Html.DisplayNameFor(model => model.RoomId)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.ItemId)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.QuantityConsumed)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.ConsumedDate)
            </th>
            <th></th>
        </tr>
    </thead>
    <tbody>
@foreach (var item in Model) {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.RoomId)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.ItemId)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.QuantityConsumed)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.ConsumedDate)
            </td>
            <td>
                <a asp-action="Edit" asp-route-id="@item.ConsumptionId">Edit</a> |
                <a asp-action="Details" asp-route-id="@item.ConsumptionId">Details</a> |
                <a asp-action="Delete" asp-route-id="@item.ConsumptionId">Delete</a>
            </td>
        </tr>
}
    </tbody>
</table>
