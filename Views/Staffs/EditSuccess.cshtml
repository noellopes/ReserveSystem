@model ReserveSystem.Models.Staff

@{
    ViewData["Title"] = "Edit Success";
}

<h2>Staff Edited Successfully</h2>

<div>
    <h3>Staff Information</h3>
    <dl class="row">
        <dt class="col-sm-2">Name</dt>
        <dd class="col-sm-10">@Model.StaffName</dd>

        <dt class="col-sm-2">Email</dt>
        <dd class="col-sm-10">@Model.StaffEmail</dd>

        <dt class="col-sm-2">Phone</dt>
        <dd class="col-sm-10">@Model.StaffPhone</dd>

        <dt class="col-sm-2">Job</dt>
        <dd class="col-sm-10">@Model.job.JobDescription</dd>
    </dl>
</div>

<p>
    <a href="Index" class="btn btn-primary">Back to Staff List</a>
</p>
