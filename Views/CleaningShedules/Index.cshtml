                @Html.DisplayNameFor(model => model.staff)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.roomBooking)
            </th>
            <th></th>
        </tr>
    </thead>
    <tbody>
@foreach (var item in Model) {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.DateServices)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.StartTime)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.EndTime)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.cleaning.CleaningId)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.staff.StaffId)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.roomBooking.RoomBookingId)
            </td>
